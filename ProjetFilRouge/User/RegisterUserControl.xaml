<UserControl x:Class="ProjetFilRouge.User.RegisterUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ProjetFilRouge.User"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Label Content="S'enregister" HorizontalAlignment="Center" VerticalAlignment="Top" Width="Auto" Height="Auto" FontSize="30"/>
        <Label Content="Username :" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,100" Width="120"/>
        <TextBox x:Name="Username" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,50" TextWrapping="Wrap"  Width="120" />
        <Label Content="Password :" HorizontalAlignment="Center" VerticalAlignment="Center" Width="120"/>
        <TextBox x:Name="Password" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,50,0,0" TextWrapping="Wrap"  Width="120" />
        <Button x:Name="Valid" Content="Valider" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,150,0,0" Width="80" Height="20" Click="Valid_Click"/>
    </Grid>
</UserControl>
